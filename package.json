{
  "name": "code-tracker",
  "displayName": "Code Tracker",
  "description": "Automatically track and save Jupyter notebook states to your API when cells are executed. Manage your client key and view tracked files through an intuitive settings panel.",
  "version": "0.0.1",
  "publisher": "Moez Missaoui",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Machine Learning"
  ],
  "keywords": [
    "jupyter",
    "notebook",
    "tracking",
    "api",
    "data-science",
    "ml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MoezMissaoui/Code-Tracker-Extension-VS-Code"
  },
  "license": "MIT",
  "icon": "icon.png",
  "activationEvents": [
    "onNotebook:jupyter-notebook",
    "onCommand:code-tracker.sendNotebookState",
    "onCommand:code-tracker.configureClientKey",
    "onCommand:code-tracker.openSettings"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-tracker.sendNotebookState",
        "title": "Code Tracker: Send Notebook State"
      },
      {
        "command": "code-tracker.configureClientKey",
        "title": "Code Tracker: Configure Client Key"
      },
      {
        "command": "code-tracker.openSettings",
        "title": "Code Tracker: Open Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "ts-loader": "^9.5.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
